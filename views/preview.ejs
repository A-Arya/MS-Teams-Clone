<body style="background-color: #232323;">
<div class="main">
    <nav class="navbar">
          <span class="navbar-text">
            Meeting
          </span>
      </nav>
    <p class="h5">Choose audio and video settings for your meeting</p>

    <div class = "main_videos">
        <div id="video-grid">

         </div>
    </div>
    <div class="main_controls">
        <div class="main_controls_block">
                <div onclick="muteUnmute()" class="main_controls_button main_mute_button">
                    <i class="fas fa-microphone"></i>
                </div>
               <div onclick="playStop()" class="main_controls_button main_video_button" >
                    <i class="fas fa-video"></i>
               </div>
        </div>
    </div>
    <div class="buttons">
        <a href="/" class="btn btn-primary btn-sm" id="one">Cancel</a>
        <a href="/preview/room" class="btn btn-secondary btn-sm" id="two">Join now</a>
    </div>

</div>
    

